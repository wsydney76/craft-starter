{
  "name": "wsydney76/craft_starter",
  "version": "1.4.3",
  "description": "Simple multilingual project template for Craft CMS",
  "license": "Unlicense",
  "require": {
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/cms": "3.5.15.1",
    "craftcms/contact-form": "2.2.7",
    "doublesecretagency/craft-cpcss": "2.2.1",
    "spicyweb/craft-embedded-assets": "2.4.3"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/",
      "config\\": "config/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('config/Env.php') || copy('config/Env.php.example', 'config/Env.php');\""
    ],
    "pre-update-cmd": [
      "@php craft off --retry=30"
    ],
    "post-update-cmd": [
      "@php craft clear-caches/all",
      "@php craft migrate/all",
      "@php craft on"
    ]
  }
}
