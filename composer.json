{
  "name": "wsydney76/craft_starter",
  "version": "1.1",
  "description": "Simple multilingual project template for Craft CMS",
  "license": "Unlicense",
  "require": {
    "clubstudioltd/craft-asset-rev": "6.0.2",
    "craftcms/cms": "3.5.x-dev",
    "doublesecretagency/craft-cpcss": "^2.2",
    "spicyweb/craft-embedded-assets": "^2.2"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "project\\": "project/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@php -r \"file_exists('project/Env.php') || copy('project/Env.php.example', 'project/Env.php');\""
    ]
  }
}
